[color]
    ui = true
    branch = true
    diff = true
    interactive = true
    status = true
[color "status"]
    added = green
    changed = red
    deleted = red
    untracked = yellow
[alias]
    hist = log --all --decorate --oneline --graph
    co = checkout
    staged = diff --name-only --cached
[push]
    default = current
[pull]
    rebase = false
[init]
  defaultBranch = main
[include]
  path = ~/.gitcustom/gitcustom

